			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "ds_adls_2021_data",
								"type": "DatasetReference"
							},
							"name": "SourceADLS"
						}
					],
					"sinks": [
